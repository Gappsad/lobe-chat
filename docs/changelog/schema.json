{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "versionRange": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "minItems": 1,
        "maxItems": 2
      },
      "type": {
        "type": "string",
        "enum": ["all", "cloud", "public"]
      }
    },
    "required": ["id", "versionRange", "type"]
  },
  "type": "array"
}
